{
  "groups": [
    {
      "name": "basic-security",
      "type": "com.github.apycazo.pylon.core.security.SecurityHandler",
      "sourceType": "com.github.apycazo.pylon.core.security.SecurityHandler"
    },
    {
      "name": "pylon.rest",
      "type": "com.github.apycazo.pylon.core.rest.RestTemplateConfig",
      "sourceType": "com.github.apycazo.pylon.core.rest.RestTemplateConfig"
    }
  ],
  "properties": [
    {
      "name": "basic-security.login-header",
      "type": "java.lang.String",
      "sourceType": "com.github.apycazo.pylon.core.security.SecurityHandler",
      "defaultValue": "X-Login"
    },
    {
      "name": "basic-security.login-restricted",
      "type": "java.lang.Boolean",
      "description": "By default, login must be done from the login endpoint. If true, any endpoint can login.",
      "sourceType": "com.github.apycazo.pylon.core.security.SecurityHandler",
      "defaultValue": true
    },
    {
      "name": "basic-security.policies",
      "type": "java.util.List<com.github.apycazo.pylon.core.security.SecurityPolicy>",
      "sourceType": "com.github.apycazo.pylon.core.security.SecurityHandler"
    },
    {
      "name": "basic-security.session-idle-time-in-seconds",
      "type": "java.lang.Integer",
      "sourceType": "com.github.apycazo.pylon.core.security.SecurityHandler",
      "defaultValue": 300
    },
    {
      "name": "basic-security.token-header",
      "type": "java.lang.String",
      "sourceType": "com.github.apycazo.pylon.core.security.SecurityHandler",
      "defaultValue": "X-Token"
    }
  ],
  "hints": []
}